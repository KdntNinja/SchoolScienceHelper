package community

import "KdnSite/ui/layouts"
import "KdnSite/ui/components/card"

templ Leaderboard() {
	@layouts.BaseLayout() {
		@card.Card(card.Props{Class: "container mx-auto py-12 max-w-2xl w-full"}) {
			@card.Title(card.TitleProps{Class: "text-3xl font-bold mb-6 text-primary"}) {
				Leaderboard
			}
			<p class="text-muted-foreground">Leaderboard coming soon.</p>
		}
	}
}
