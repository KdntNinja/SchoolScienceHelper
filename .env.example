# Example environment variables for local development
# Neon (PostgreSQL) connection string
NEON_DATABASE_URL=postgres://user:password@ep-xxxxxx.eu-central-1.aws.neon.tech/dbname?sslmode=require

# Auth0 configuration
AUTH0_CLIENT_ID=your-auth0-client-id
AUTH0_DOMAIN=your-auth0-domain

# Session keys (generate securely for production)
SESSION_HASH_KEY=your_32_byte_base64_hash_key
SESSION_BLOCK_KEY=your_32_byte_base64_block_key

# App environment
GO_ENV=development
PORT=8090